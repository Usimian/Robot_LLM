            </box>
          </geometry>
          <material>
            <ambient>0.6 0.6 0.7 1</ambient>
            <diffuse>0.6 0.6 0.7 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- Internal maze walls - Creates S-shaped path with dead ends -->
    <!-- Starting chamber wall (robot starts here, must go forward then turn) -->
    <model name='maze_wall_1'>
      <pose>1.2 2 0.75 0 0 0</pose>
      <static>true</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <box>
              <size>0.2 2.5 1.5</size>
            </box>
          </geometry>
        </collision>
        <visual name='visual'>
          <geometry>
            <box>
              <size>0.2 2.5 1.5</size>
            </box>
          </geometry>
          <material>
            <ambient>0.7 0.5 0.3 1</ambient>
            <diffuse>0.7 0.5 0.3 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- First corridor divider -->
    <model name='maze_wall_2'>
      <pose>-1.2 1 0.75 0 0 0</pose>
      <static>true</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <box>
              <size>0.2 3.5 1.5</size>
            </box>
          </geometry>
        </collision>
        <visual name='visual'>
          <geometry>
            <box>
              <size>0.2 3.5 1.5</size>
            </box>
          </geometry>
          <material>
            <ambient>0.7 0.5 0.3 1</ambient>
            <diffuse>0.7 0.5 0.3 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- Center divider creating main corridor -->
    <model name='maze_wall_3'>
      <pose>0 -1 0.75 0 0 0</pose>
      <static>true</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <box>
              <size>3.5 0.2 1.5</size>
            </box>
          </geometry>
        </collision>
        <visual name='visual'>
          <geometry>
            <box>
              <size>3.5 0.2 1.5</size>
            </box>
          </geometry>
          <material>
            <ambient>0.7 0.5 0.3 1</ambient>
            <diffuse>0.7 0.5 0.3 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- Right side barrier -->
    <model name='maze_wall_4'>
      <pose>2.5 -2.5 0.75 0 0 0</pose>
      <static>true</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <box>
              <size>0.2 3 1.5</size>
            </box>
          </geometry>
        </collision>
        <visual name='visual'>
          <geometry>
            <box>
              <size>0.2 3 1.5</size>
            </box>
          </geometry>
          <material>
            <ambient>0.7 0.5 0.3 1</ambient>
            <diffuse>0.7 0.5 0.3 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- Dead-end trap -->
    <model name='maze_wall_5'>
      <pose>-2.5 -2.5 0.75 0 0 0</pose>
      <static>true</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <box>
              <size>0.2 2 1.5</size>
            </box>
          </geometry>
        </collision>
        <visual name='visual'>
          <geometry>
            <box>
              <size>0.2 2 1.5</size>
            </box>
          </geometry>
          <material>
            <ambient>0.7 0.5 0.3 1</ambient>
            <diffuse>0.7 0.5 0.3 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- Exit corridor gate -->
    <model name='maze_wall_6'>
      <pose>-1 -2 0.75 0 0 0</pose>
      <static>true</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <box>
              <size>2.5 0.2 1.5</size>
            </box>
          </geometry>
        </collision>
        <visual name='visual'>
          <geometry>
            <box>
              <size>2.5 0.2 1.5</size>
            </box>
          </geometry>
          <material>
            <ambient>0.7 0.5 0.3 1</ambient>
            <diffuse>0.7 0.5 0.3 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- EXIT MARKER (bright green box at maze exit) -->
    <model name='maze_exit_marker'>
      <pose>-3 -3 0.5 0 0 0</pose>
      <static>true</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <box>
              <size>0.5 0.5 1</size>
            </box>
          </geometry>
        </collision>
        <visual name='visual'>
          <geometry>
            <box>
              <size>0.5 0.5 1</size>
            </box>
          </geometry>
          <material>
            <ambient>0.0 1.0 0.0 1</ambient>
            <diffuse>0.0 1.0 0.0 1</diffuse>
            <emissive>0.0 0.5 0.0 1</emissive>
          </material>
        </visual>
      </link>
    </model>

    <model name='vehicle_blue' xmlns:gz="http://gazebosim.org/schema">
      <pose>0 2 0.325 0 -0 0</pose>

      <link name='chassis'>
        <pose>0 0 0.0815 0 0 0</pose>
        <inertial>
          <mass>0.486</mass>
          <inertia>
            <ixx>0.002</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.005</iyy>
            <iyz>0</iyz>
            <izz>0.005</izz>
          </inertia>
        </inertial>
        <visual name='visual'>
          <geometry>
            <box>
              <size>0.2 0.15 0.08</size>
            </box>
          </geometry>
          <material>
            <ambient>0 0.7 0 1</ambient>
            <diffuse>0 0.7 0 1</diffuse>
            <specular>0.1 0.1 0.1 1</specular>
          </material>
        </visual>
        <collision name='collision'>
          <geometry>
            <box>
              <size>0.2 0.15 0.08</size>
            </box>
          </geometry>
        </collision>
      </link>

      <link name='front_left_wheel'>
        <pose relative_to='chassis'>0.06 0.0775 -0.0165 -1.5707 0 0</pose>
        <inertial>
          <mass>0.05</mass>
          <inertia>
            <ixx>0.000018</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.000018</iyy>
            <iyz>0</iyz>
            <izz>0.000027</izz>
          </inertia>
        </inertial>
        <visual name='visual'>
          <pose>0 0 0 1.5707 0 0</pose>
          <geometry>
            <!-- scale mesh to radius == 0.033 -->
            <mesh>
              <uri>https://fuel.gazebosim.org/1.0/OpenRobotics/models/Mecanum lift/tip/files/meshes/mecanum_wheel_left.STL</uri>
              <scale>0.00066 0.00066 0.00066</scale>
            </mesh>
          </geometry>
          <material>
            <ambient>0.2 0.2 0.2 1</ambient>
            <diffuse>0.2 0.2 0.2 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
          </material>
        </visual>
        <collision name='collision'>
          <geometry>
            <sphere>
              <radius>0.033</radius>
            </sphere>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>1.0</mu>
                <mu2>0.0</mu2>
                <fdir1 gz:expressed_in="chassis">1 -1 0</fdir1>
              </ode>
            </friction>
          </surface>
        </collision>
      </link>

      <link name='rear_left_wheel'>
        <pose relative_to='chassis'>-0.06 0.0775 -0.0165 -1.5707 0 0</pose>
        <inertial>
          <mass>0.05</mass>
          <inertia>
            <ixx>0.000018</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.000018</iyy>
            <iyz>0</iyz>
            <izz>0.000027</izz>
          </inertia>
        </inertial>
        <visual name='visual'>
          <pose>0 0 0 1.5707 0 0</pose>
          <geometry>
            <!-- scale mesh to radius == 0.033 -->
            <mesh>
              <uri>https://fuel.gazebosim.org/1.0/OpenRobotics/models/Mecanum lift/tip/files/meshes/mecanum_wheel_right.STL</uri>
              <scale>0.00066 0.00066 0.00066</scale>
            </mesh>
          </geometry>
          <material>
            <ambient>0.2 0.2 0.2 1</ambient>
            <diffuse>0.2 0.2 0.2 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
          </material>
        </visual>
        <collision name='collision'>
          <geometry>
            <sphere>
              <radius>0.033</radius>
            </sphere>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>1.0</mu>
                <mu2>0.0</mu2>
                <fdir1 gz:expressed_in="chassis">1 1 0</fdir1>
              </ode>
            </friction>
          </surface>
        </collision>
      </link>

      <link name='front_right_wheel'>
        <pose relative_to='chassis'>0.06 -0.0775 -0.0165 -1.5707 0 0</pose>
        <inertial>
          <mass>0.05</mass>
          <inertia>
            <ixx>0.000018</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.000018</iyy>
            <iyz>0</iyz>
            <izz>0.000027</izz>
          </inertia>
        </inertial>
        <visual name='visual'>
          <pose>0 0 0 1.5707 0 0</pose>
          <geometry>
            <!-- scale mesh to radius == 0.033 -->
            <mesh>
              <uri>https://fuel.gazebosim.org/1.0/OpenRobotics/models/Mecanum lift/tip/files/meshes/mecanum_wheel_right.STL</uri>
              <scale>0.00066 0.00066 0.00066</scale>
            </mesh>
          </geometry>
          <material>
            <ambient>0.2 0.2 0.2 1</ambient>
            <diffuse>0.2 0.2 0.2 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
          </material>
        </visual>
        <collision name='collision'>
          <geometry>
            <sphere>
              <radius>0.033</radius>
            </sphere>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>1.0</mu>
                <mu2>0.0</mu2>
                <fdir1 gz:expressed_in="chassis">1 1 0</fdir1>
              </ode>
            </friction>
          </surface>
        </collision>
      </link>

      <link name='rear_right_wheel'>
        <pose relative_to='chassis'>-0.06 -0.0775 -0.0165 -1.5707 0 0</pose>
        <inertial>
          <mass>0.05</mass>
          <inertia>
            <ixx>0.000018</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.000018</iyy>
            <iyz>0</iyz>
            <izz>0.000027</izz>
          </inertia>
        </inertial>
        <visual name='visual'>
          <pose>0 0 0 1.5707 0 0</pose>
          <geometry>
            <!-- scale mesh to radius == 0.033 -->
            <mesh>
              <uri>https://fuel.gazebosim.org/1.0/OpenRobotics/models/Mecanum lift/tip/files/meshes/mecanum_wheel_left.STL</uri>
              <scale>0.00066 0.00066 0.00066</scale>
            </mesh>
          </geometry>
          <material>
            <ambient>0.2 0.2 0.2 1</ambient>
            <diffuse>0.2 0.2 0.2 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
          </material>
        </visual>
        <collision name='collision'>
          <geometry>
            <sphere>
              <radius>0.033</radius>
            </sphere>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>1.0</mu>
                <mu2>0.0</mu2>
                <fdir1 gz:expressed_in="chassis">1 -1 0</fdir1>
              </ode>
            </friction>
          </surface>
        </collision>
      </link>


      <joint name='front_left_wheel_joint' type='revolute'>
        <parent>chassis</parent>
        <child>front_left_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1.79769e+308</lower>
            <upper>1.79769e+308</upper>
          </limit>
        </axis>
      </joint>

      <joint name='front_right_wheel_joint' type='revolute'>
        <parent>chassis</parent>
        <child>front_right_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1.79769e+308</lower>
            <upper>1.79769e+308</upper>
          </limit>
        </axis>
      </joint>

      <joint name='rear_left_wheel_joint' type='revolute'>
        <parent>chassis</parent>
        <child>rear_left_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1.79769e+308</lower>
            <upper>1.79769e+308</upper>
          </limit>
        </axis>
      </joint>

      <joint name='rear_right_wheel_joint' type='revolute'>
        <parent>chassis</parent>
        <child>rear_right_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1.79769e+308</lower>
            <upper>1.79769e+308</upper>
          </limit>
        </axis>
      </joint>

      <!-- RGB-D Camera (combined RGB and Depth) -->
      <link name='camera_link'>
        <pose relative_to='chassis'>0.057 0.0 0.038 0 0 0</pose>
        <inertial>
          <mass>0.02</mass>
          <inertia>
            <ixx>0.000002</ixx>
            <iyy>0.000002</iyy>
            <izz>0.000001</izz>
          </inertia>
        </inertial>
        <visual name='visual'>
          <geometry>
            <box>
              <size>0.03 0.1 0.03</size>
            </box>
          </geometry>
          <material>
            <ambient>0.2 0.2 0.8 1</ambient>
            <diffuse>0.2 0.2 0.8 1</diffuse>
          </material>
        </visual>
        <!-- RGB Sensor -->
        <sensor name='camera' type='camera'>
          <camera>
            <horizontal_fov>1.047</horizontal_fov>
            <image>
              <width>640</width>
              <height>480</height>
            </image>
            <clip>
              <near>0.1</near>
              <far>10</far>
            </clip>
          </camera>
          <always_on>1</always_on>
          <update_rate>30</update_rate>
          <visualize>true</visualize>
          <topic>camera</topic>
        </sensor>
        <!-- Depth Sensor -->
        <sensor name='depth_camera' type='depth_camera'>
          <camera>
            <horizontal_fov>1.047</horizontal_fov>
            <image>
              <width>640</width>
              <height>480</height>
            </image>
            <clip>
              <near>0.1</near>
              <far>10</far>
            </clip>
          </camera>
          <always_on>1</always_on>
          <update_rate>30</update_rate>
          <visualize>true</visualize>
          <topic>depth_camera</topic>
        </sensor>
      </link>

      <joint name='camera_joint' type='fixed'>
        <parent>chassis</parent>
        <child>camera_link</child>
      </joint>

      <!-- IMU -->
      <link name='imu_link'>
        <pose relative_to='chassis'>-0.06 0.01 0.01 0 0 0</pose>
        <inertial>
          <mass>0.01</mass>
          <inertia>
            <ixx>0.000001</ixx>
            <iyy>0.000001</iyy>
            <izz>0.000001</izz>
          </inertia>
        </inertial>
        <sensor name='imu_sensor' type='imu'>
          <topic>imu</topic>
          <update_rate>100</update_rate>
          <imu>
            <angular_velocity>
              <x>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>0.01</stddev>
                </noise>
              </x>
              <y>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>0.01</stddev>
                </noise>
              </y>
              <z>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>0.01</stddev>
                </noise>
              </z>
            </angular_velocity>
            <linear_acceleration>
              <x>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>0.1</stddev>
                </noise>
              </x>
              <y>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>0.1</stddev>
                </noise>
              </y>
              <z>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>0.1</stddev>
                </noise>
              </z>
            </linear_acceleration>
          </imu>
          <always_on>1</always_on>
        </sensor>
      </link>

      <joint name='imu_joint' type='fixed'>
        <parent>chassis</parent>
        <child>imu_link</child>
      </joint>

      <!-- Lidar -->
      <link name='lidar_link'>
        <pose relative_to='chassis'>0.0435 0.0001 0.11 0 0 0</pose>
        <inertial>
          <mass>0.05</mass>
          <inertia>
            <ixx>0.000021</ixx>
            <iyy>0.000021</iyy>
            <izz>0.000016</izz>
          </inertia>
        </inertial>
        <visual name='visual'>
          <geometry>
            <cylinder>
              <radius>0.025</radius>
              <length>0.04</length>
            </cylinder>
          </geometry>
          <material>
